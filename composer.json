{
    "name": "royvoetman/flysystem-gitlab-storage",
    "description": "Flysystem Adapter for the Gitlab Repository files API v4",
    "keywords": [
        "royvoetman",
        "flysystem-gitlab",
        "flysystem",
        "gitlab",
        "v4",
        "api",
        "laravel-storage"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roy Voetman",
            "email": "royvoetman@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "league/flysystem": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "RoyVoetman\\FlysystemGitlab\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RoyVoetman\\FlysystemGitlab\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
